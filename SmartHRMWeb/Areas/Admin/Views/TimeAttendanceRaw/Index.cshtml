<form method="post">
    <div class="card shadow-none border border-300 mb-3  card-default card-table" data-component-card="data-component-card">

        <div class="card-body p-0">
            <div class="collapse code-collapse" id="search-example-code">
                <pre class="scrollbar" style="max-height:420px">

            </pre>
            </div>
            <div class="p-4 code-to-copy">

                <h2 class="text-bold text-1100 mb-5">Time & Attendance Posting</h2>
                <div id="tableExample3">

                    <div class="">
                        <div class="table-responsive row">
                            <div class="">
                                <table class="table table-striped table-sm fs--1 mb-0" id="timeAttendanceRawtblbody" width="100%">
                                    <thead>
                                        <tr>         
                                           
                                            <th class="sort border-top">#</th>                                            
                                            <th class="sort border-top">Employee</th>
                                            <th class="sort border-top">Month</th>
                                            <th class="sort border-top">Year</th>
                                            <th class="sort border-top">Present(days)</th>
                                            <th class="sort border-top">Absent(days)</th>                                         
                                            <th class="sort border-top" data-sort="state">
                                                <div class="form-check mb-0 fs-0">
                                                    <input class="form-check-input sort border-top" id="checkedAll" type="checkbox" data-bulk-select='{"body":"bulk-select-body","actions":"bulk-select-actions","replacedElement":"bulk-select-replace-element"}' />Posted
                                                </div>
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody class="list">
                                    </tbody>
                                </table>
                                <br />
                                <ul class="list-group mb-3">
                                </ul>
                            </div>

                            <div class="flex-1 text-end">
                                <button class="btn btn-primary px-6 px-sm-6" id="btnpostdata" onclick="return ValidateInput()" data-wizard-next-btn="data-wizard-next-btn">Post Data<span class="fas fa-chevron-right ms-1" data-fa-transform="shrink-3"> </span></button>
                            </div>

                        </div>

                    </div>

                </div>
            </div>



        </div>
    </div>
    </div>
</form>


@section Scripts{
    <script src="~/js/timeAndAttendanceRawFinal.js"></script>
}



